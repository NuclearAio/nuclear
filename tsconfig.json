{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client", "node"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "src/pages/Reports.jsx", "src/pages/Tasks.jsx", "src/pages/Subscription.jsx", "src/pages/Settings.jsx", "src/pages/Proxies.jsx", "src/pages/Profiles.jsx", "src/pages/Login.jsx", "src/pages/Cards.jsx", "src/pages/Dashboard.jsx", "src/pages/Dashboard.jsx"]
}
